{
  "manifest_version": 3,
  "name": "ShieldVPN ProxyMaster by T00THL3$$",
  "version": "1.0",
  "description": "Mask your IP address using custom HTTP proxies.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
